# Список каналов для мониторинга (через запятую)
CHANNELS=omyinvestments,alfa_investments,tb_invest_official,SberInvestments,centralbank_russia,selfinvestor

# Интервал повторных попыток отправки неудачных постов в секундах
RETRY_INTERVAL=60

# URL для отправки чанков в Retriever API (опционально)
WEBHOOK_URL=http://localhost:8010/retriever/documents

# Telegram API credentials
# Получите на https://my.telegram.org
API_ID=
API_HASH=
PHONE=

# Имя файла сессии Telegram (используется если TELEGRAM_SESSION_STRING не указан)
SESSION_NAME=my_session

# Строка сессии Telegram (если указана, используется вместо файла сессии)
# Получить можно через скрипт authorize_telegram.py
TELEGRAM_SESSION_STRING=

# Директория для хранения данных
DATA_DIR=data

# LLM провайдер для определения актуальности постов
# Доступные значения: qwen, yandexgpt, chatgpt, deepseek
LLM_PROVIDER=qwen

# LLM Client API настройки
LLM_CLIENT_API_URL=http://localhost:8014

# Qdrant настройки для удаления устаревших постов
# Если не указаны, механизм удаления постов не будет работать
QDRANT_HOST=
QDRANT_PORT=
QDRANT_API_KEY=
QDRANT_COLLECTION_NAME=
QDRANT_TIMEOUT=60
